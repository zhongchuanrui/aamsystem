<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.aim.dao.AcommentDao">

	<insert id="insertAcomment" parameterType="com.aim.pojo.Acomment">
		insert into acomment(acomment_manid,acomment_man,acomment_content,acomment_writetime,acomment_replaymanid,acomment_replayman,acomment_state)
		values(#{acomment_manid},#{acomment_man},#{acomment_content},#{acomment_writetime},#{acomment_replaymanid},#{acomment_replayman},1)
	</insert>

	
</mapper>